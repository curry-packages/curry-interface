{
  "name": "curry-interface",
  "version": "1.0.0",
  "author": "Dennis Thomsen <stu232874@mail.uni-kiel.de>",
  "maintainer": "Michael Hanus <mh@informatik.uni-kiel.de>",
  "synopsis": "Libraries to represent and read module interfaces of Curry programs",
