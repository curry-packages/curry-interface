{
  "name": "curry-interface",
  "version": "2.0.1",
  "author": "Dennis Thomsen <stu232874@mail.uni-kiel.de>",
  "maintainer": "Michael Hanus <mh@informatik.uni-kiel.de>",
  "synopsis": "Libraries to represent and read module interfaces of Curry programs",
  "category": [ "Metaprogramming" ],
  "license": "BSD-3-Clause",
  "licenseFile": "LICENSE",
  "dependencies": {
      "base"          : ">= 3.2.0, < 4.0.0",
      "currypath"     : ">= 3.0.0, < 4.0.0",
      "det-parse"     : ">= 3.0.0, < 4.0.0",
      "directory"     : ">= 3.0.0, < 4.0.0",
      "filepath"      : ">= 3.0.0, < 4.0.0",
      "frontend-exec" : ">= 3.0.0, < 4.0.0",
      "process"       : ">= 3.0.0, < 4.0.0",
      "wl-pprint"     : ">= 3.0.0, < 4.0.0"
  },
  "compilerCompatibility": {
      "pakcs"   : ">= 3.8.0, < 4.0.0",
      "kics2"   : ">= 3.3.0, < 4.0.0",
      "curry2go": ">= 1.5.0, < 2.0.0",
      "kmcc"    : ">= 0.3.0, < 2.0.0"
  },
  "exportedModules": [ "CurryInterface.Files", "CurryInterface.Pretty"
                     , "CurryInterface.Types" ],
  "executable": {
      "name": "curry-showinterface",
      "main": "ShowInterface"
  },
  "source": {
      "git": "https://github.com/curry-packages/curry-interface.git",
      "tag": "$version"
  },
  "testsuite": {
      "src-dir": "examples",
      "modules": [ "Test" ]
  }
}
